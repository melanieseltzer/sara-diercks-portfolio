{
  "name": "sara-diercks-portfolio",
  "version": "0.1.0",
  "author": "Melanie Seltzer <melleh11@gmail.com>",
  "description": "GIS portfolio for Sara Diercks",
  "repository": "https://github.com/melanieseltzer/sara-diercks-portfolio",
  "license": "MIT",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "lint": "eslint 'pages/**/*.js' 'src/**/*.js'",
    "prettier": "prettier --write",
    "flow": "flow",
    "deploy": "now rm sara-diercks-portfolio --safe --yes ; now && now alias"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn flow"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npm run prettier",
      "eslint --ignore-path .eslintignore",
      "git add"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.12",
    "@fortawesome/free-solid-svg-icons": "^5.6.3",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "apollo-boost": "^0.1.23",
    "graphql": "^14.0.2",
    "next": "^7.0.2",
    "next-with-apollo": "^3.3.1",
    "react": "^16.6.3",
    "react-apollo": "^2.3.3",
    "react-burgers": "^1.1.1",
    "react-dom": "^16.6.3",
    "styled-components": "^4.1.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-plugin-styled-components": "^1.9.2",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "flow-bin": "^0.87.0",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3"
  }
}
